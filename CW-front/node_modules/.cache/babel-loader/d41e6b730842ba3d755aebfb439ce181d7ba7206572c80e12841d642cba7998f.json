{"ast":null,"code":"import companyService from \"@/services/company.service\";\nimport UserList from \"../UserList.vue\";\nimport ClientList from \"../ClientList.vue\";\nexport default {\n  components: {\n    UserList,\n    ClientList\n  },\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: \"Companies\",\n  data() {\n    return {\n      companies: [],\n      currentCompany: null,\n      currentId: -1,\n      users: [],\n      clients: []\n    };\n  },\n  methods: {\n    getAllCompanies() {\n      companyService.getAllCompanies().then(response => {\n        this.companies = response.data;\n      }, error => {\n        this.companies = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n      });\n    },\n    setActiveCompany(company, currentId) {\n      this.currentCompany = company;\n      this.currentId = company ? currentId : -1;\n      this.getUsersByCompId(company.id);\n      this.getClients(company.id);\n    },\n    getUsersByCompId(currentId) {\n      companyService.getUsersByCompanyId(currentId).then(response => {\n        this.users = response.data;\n      }, error => {\n        this.users = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n      });\n      this.users = this.users.filter(v => v.id !== this.currentUser.id);\n      console.log();\n    },\n    getClients(currentId) {\n      companyService.getClientsByCompany(currentId).then(response => {\n        this.clients = response.data;\n      }, error => {\n        this.clients = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n      });\n    }\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  mounted() {\n    this.getAllCompanies();\n  }\n};","map":{"version":3,"names":["companyService","UserList","ClientList","components","name","data","companies","currentCompany","currentId","users","clients","methods","getAllCompanies","then","response","error","message","toString","setActiveCompany","company","getUsersByCompId","id","getClients","getUsersByCompanyId","filter","v","currentUser","console","log","getClientsByCompany","computed","$store","state","auth","user","mounted"],"sources":["C:\\Users\\Admin\\Desktop\\X\\SSTU\\6 семак\\БД\\DBCourseWork\\CW-front\\src\\components\\pages\\CompaniesPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"list row\">\r\n    <div class=\"col-md-3\">\r\n      <br />\r\n      <h4>Список компаний:</h4>\r\n      <ul class=\"list-group\">\r\n        <li\r\n          class=\"companyItem list-group-item\"\r\n          :class=\"{ active: id == currentId }\"\r\n          v-for=\"(company, id) in companies\"\r\n          :key=\"company.id\"\r\n          @click=\"setActiveCompany(company, currentId)\"\r\n        >\r\n          {{ company.name }}\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"col-md-9\">\r\n      <br />\r\n      <div v-if=\"currentCompany\">\r\n        <h4>Выбранная компания</h4>\r\n        <div>\r\n          <label><strong>Name: </strong></label> {{ currentCompany.name }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Description: </strong></label>\r\n          {{ currentCompany.description }}\r\n        </div>\r\n        <div class=\"container lists\">\r\n          <user-list class=\"usList\" :users=\"users\" @remove=\"removeUser\"></user-list>\r\n          <client-list :clients=\"clients\"></client-list>\r\n        </div>\r\n        <!-- <router-link :to=\"'/tutorials/' + currentTutorial.id\" class=\"badge badge-warning\">Edit</router-link> -->\r\n      </div>\r\n      <div v-else>\r\n        <br />\r\n        <p>Please click on a Company...</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport companyService from \"@/services/company.service\";\r\nimport UserList from \"../UserList.vue\";\r\nimport ClientList from \"../ClientList.vue\";\r\n\r\nexport default {\r\n  components: { UserList, ClientList },\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: \"Companies\",\r\n  data() {\r\n    return {\r\n      companies: [],\r\n      currentCompany: null,\r\n      currentId: -1,\r\n      users: [],\r\n      clients: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    getAllCompanies() {\r\n      companyService.getAllCompanies().then(\r\n        (response) => {\r\n          this.companies = response.data;\r\n        },\r\n        (error) => {\r\n          this.companies =\r\n            (error.response &&\r\n              error.response.data &&\r\n              error.response.data.message) ||\r\n            error.message ||\r\n            error.toString();\r\n        }\r\n      );\r\n    },\r\n    setActiveCompany(company, currentId) {\r\n      this.currentCompany = company;\r\n      this.currentId = company ? currentId : -1;\r\n      this.getUsersByCompId(company.id);\r\n      this.getClients(company.id);\r\n    },\r\n\r\n    getUsersByCompId(currentId) {\r\n      companyService.getUsersByCompanyId(currentId).then(\r\n        (response) => {\r\n          this.users = response.data;\r\n        },\r\n        (error) => {\r\n          this.users =\r\n            (error.response &&\r\n              error.response.data &&\r\n              error.response.data.message) ||\r\n            error.message ||\r\n            error.toString();\r\n        }\r\n      );\r\n      this.users = this.users.filter((v) => v.id !== this.currentUser.id);\r\n      console.log();\r\n    },\r\n\r\n    getClients(currentId) {\r\n      companyService.getClientsByCompany(currentId).then(\r\n        (response) => {\r\n          this.clients = response.data;\r\n        },\r\n        (error) => {\r\n          this.clients =\r\n            (error.response &&\r\n              error.response.data &&\r\n              error.response.data.message) ||\r\n            error.message ||\r\n            error.toString();\r\n        }\r\n      );\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    this.getAllCompanies();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.list {\r\n  text-align: left;\r\n  margin: auto;\r\n}\r\n.companyItem {\r\n  border: 2px solid teal;\r\n  border-radius: 5px;\r\n}\r\n.lists {\r\n  display: flex;\r\n}\r\n.usList{\r\n  margin-top: 0px;\r\n}\r\n</style>"],"mappings":"AA2CA,OAAOA,cAAa,MAAO,4BAA4B;AACvD,OAAOC,QAAO,MAAO,iBAAiB;AACtC,OAAOC,UAAS,MAAO,mBAAmB;AAE1C,eAAe;EACbC,UAAU,EAAE;IAAEF,QAAQ;IAAEC;EAAW,CAAC;EACpC;EACAE,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,cAAc,EAAE,IAAI;MACpBC,SAAS,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChBZ,cAAc,CAACY,eAAe,CAAC,CAAC,CAACC,IAAI,CAClCC,QAAQ,IAAK;QACZ,IAAI,CAACR,SAAQ,GAAIQ,QAAQ,CAACT,IAAI;MAChC,CAAC,EACAU,KAAK,IAAK;QACT,IAAI,CAACT,SAAQ,GACVS,KAAK,CAACD,QAAO,IACZC,KAAK,CAACD,QAAQ,CAACT,IAAG,IAClBU,KAAK,CAACD,QAAQ,CAACT,IAAI,CAACW,OAAO,IAC7BD,KAAK,CAACC,OAAM,IACZD,KAAK,CAACE,QAAQ,CAAC,CAAC;MACpB,CACF,CAAC;IACH,CAAC;IACDC,gBAAgBA,CAACC,OAAO,EAAEX,SAAS,EAAE;MACnC,IAAI,CAACD,cAAa,GAAIY,OAAO;MAC7B,IAAI,CAACX,SAAQ,GAAIW,OAAM,GAAIX,SAAQ,GAAI,CAAC,CAAC;MACzC,IAAI,CAACY,gBAAgB,CAACD,OAAO,CAACE,EAAE,CAAC;MACjC,IAAI,CAACC,UAAU,CAACH,OAAO,CAACE,EAAE,CAAC;IAC7B,CAAC;IAEDD,gBAAgBA,CAACZ,SAAS,EAAE;MAC1BR,cAAc,CAACuB,mBAAmB,CAACf,SAAS,CAAC,CAACK,IAAI,CAC/CC,QAAQ,IAAK;QACZ,IAAI,CAACL,KAAI,GAAIK,QAAQ,CAACT,IAAI;MAC5B,CAAC,EACAU,KAAK,IAAK;QACT,IAAI,CAACN,KAAI,GACNM,KAAK,CAACD,QAAO,IACZC,KAAK,CAACD,QAAQ,CAACT,IAAG,IAClBU,KAAK,CAACD,QAAQ,CAACT,IAAI,CAACW,OAAO,IAC7BD,KAAK,CAACC,OAAM,IACZD,KAAK,CAACE,QAAQ,CAAC,CAAC;MACpB,CACF,CAAC;MACD,IAAI,CAACR,KAAI,GAAI,IAAI,CAACA,KAAK,CAACe,MAAM,CAAEC,CAAC,IAAKA,CAAC,CAACJ,EAAC,KAAM,IAAI,CAACK,WAAW,CAACL,EAAE,CAAC;MACnEM,OAAO,CAACC,GAAG,CAAC,CAAC;IACf,CAAC;IAEDN,UAAUA,CAACd,SAAS