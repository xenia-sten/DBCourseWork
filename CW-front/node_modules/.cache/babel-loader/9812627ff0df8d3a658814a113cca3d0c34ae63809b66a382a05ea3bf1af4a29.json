: { MyInput },\r\n  data() {\r\n    return {\r\n      emailQuery: \"\",\r\n      users: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    bindUserWithCompanyId() {\r\n      this.$emit(\"create\", this.user);\r\n      this.user\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    sortByEmailUsers() {\r\n      return this.users.filter((user) => user.email.includes(this.emailQuery));\r\n    },\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    userService.getAllUsers().then(\r\n      (response) => {\r\n        this.users = response.data;\r\n      }\r\n    ).catch(e => console.log(e));\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nform {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n</style>"],"mappings":"AAqBA,OAAOA,WAAU,MAAO,yBAAyB;AACjD,OAAOC,OAAM,MAAO,kBAAkB;AACtC,eAAe;EACbC,UAAU,EAAE;IAAED;EAAQ,CAAC;EACvBE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,qBAAqBA,CAAA,EAAG;MACtB,IAAI,CAACC,KAAK,CAAC,QAAQ,EAAE,IAAI,CAACC,IAAI,CAAC;MAC/B,IAAI,CAACA,IAAG;IACV;EACF,CAAC;EAEDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,OAAO,IAAI,CAACN,KAAK,CAACO,MAAM,CAAEH,IAAI,IAAKA,IAAI,CAACI,KAAK,CAACC,QAAQ,CAAC,IAAI,CAACV,UAAU,CAAC,CAAC;IAC1E,CAAC;IACDW,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACT,IAAI;IACpC;EACF,CAAC;EAEDU,OAAOA,CAAA,EAAG;IACRnB,WAAW,CAACoB,WAAW,CAAC,CAAC,CAACC,IAAI,CAC3BC,QAAQ,IAAK;MACZ,IAAI,CAACjB,KAAI,GAAIiB,QAAQ,CAACnB,IAAI;IAC5B,CACF,CAAC,CAACoB,KAAK,CAACC,CAAA,IAAKC,OAAO,CAACC,GAAG,CAACF,CAAC,CAAC,CAAC;EAC9B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}