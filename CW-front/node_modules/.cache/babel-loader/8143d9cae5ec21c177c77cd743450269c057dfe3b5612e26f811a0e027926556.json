{"ast":null,"code":"import companyService from \"@/services/company.service\";\nimport UserList from \"../UserList.vue\";\nimport ClientList from \"../ClientList.vue\";\nimport userService from \"@/services/user.service\";\nexport default {\n  components: {\n    UserList,\n    ClientList\n  },\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: \"Companies\",\n  data() {\n    return {\n      companies: [],\n      currentCompany: null,\n      currentId: -1,\n      users: [],\n      clients: []\n    };\n  },\n  methods: {\n    getAllCompanies() {\n      userService.getCompaniesByUser(this.currentUser.id).then(response => {\n        this.companies = response.data;\n        if (response.status == 404) {\n          this.companies = [];\n        }\n      }, error => {\n        console.log();\n      });\n    },\n    setActiveCompany(company, currentId) {\n      this.currentCompany = company;\n      this.currentId = company ? currentId : -1;\n      this.getUsersByCompId(company.id);\n      this.getClients(company.id);\n    },\n    getUsersByCompId(currentId) {\n      companyService.getUsersByCompanyId(currentId).then(response => {\n        this.users = response.data;\n      }, error => {\n        this.users = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n      });\n      this.users = this.users.filter(v => v.id !== this.currentUser.id);\n      console.log(this.users);\n    },\n    getClients(currentId) {\n      companyService.getClientsByCompany(currentId).then(response => {\n        this.clients = response.data;\n      }, error => {\n        this.clients = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n      });\n    }\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  mounted() {\n    this.getAllCompanies();\n  }\n};","map":{"version":3,"names":["companyService","UserList","ClientList","userService","components","name","data","companies","currentCompany","currentId","users","clients","methods","getAllCompanies","getCompaniesByUser","currentUser","id","then","response","status","error","console","log","setActiveCompany","company","getUsersByCompId","getClients","getUsersByCompanyId","message","toString","filter","v","getClientsByCompany","computed","$store","state","auth","user","mounted"],"sources":["C:\\Users\\Admin\\Desktop\\X\\SSTU\\6 семак\\БД\\DBCourseWork\\CW-front\\src\\components\\pages\\CompaniesPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"list row\">\r\n    <div class=\"col-md-3\">\r\n      <br />\r\n      <h4>Список компаний:</h4>\r\n      <ul class=\"list-group\" v-if=\"this.companies.length > 0\">\r\n        <li\r\n          class=\"companyItem list-group-item\"\r\n          :class=\"{ active: id == currentId }\"\r\n          v-for=\"(company, id) in companies\"\r\n          :key=\"company.id\"\r\n          @click=\"setActiveCompany(company, currentId)\"\r\n        >\r\n          {{ company.name }}\r\n        </li>\r\n      </ul>\r\n      <h2 v-else style=\"color: red\">Список визитов пуст</h2>\r\n    </div>\r\n    <div class=\"col-md-9\">\r\n      <br />\r\n      <div v-if=\"currentCompany\">\r\n        <h4>Выбранная компания</h4>\r\n        <div>\r\n          <label><strong>Name: </strong></label> {{ currentCompany.name }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Description: </strong></label>\r\n          {{ currentCompany.description }}\r\n        </div>\r\n        <div class=\"container lists\">\r\n          <user-list class=\"usList\" :users=\"users\" @remove=\"removeUser\"></user-list>\r\n          <client-list :clients=\"clients\"></client-list>\r\n        </div>\r\n        <!-- <router-link :to=\"'/tutorials/' + currentTutorial.id\" class=\"badge badge-warning\">Edit</router-link> -->\r\n      </div>\r\n      <div v-else>\r\n        <br />\r\n        <p>Please click on a Company...</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport companyService from \"@/services/company.service\";\r\nimport UserList from \"../UserList.vue\";\r\nimport ClientList from \"../ClientList.vue\";\r\nimport userService from \"@/services/user.service\";\r\n\r\nexport default {\r\n  components: { UserList, ClientList },\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: \"Companies\",\r\n  data() {\r\n    return {\r\n      companies: [],\r\n      currentCompany: null,\r\n      currentId: -1,\r\n      users: [],\r\n      clients: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    getAllCompanies() {\r\n      userService.getCompaniesByUser(this.currentUser.id).then(\r\n        (response) => {\r\n          this.companies = response.data;\r\n          if(response.status == 404){\r\n            this.companies = [];\r\n          }\r\n        },\r\n        (error) => {\r\n          console.log();\r\n        }\r\n      );\r\n      \r\n    },\r\n\r\n    setActiveCompany(company, currentId) {\r\n      this.currentCompany = company;\r\n      this.currentId = company ? currentId : -1;\r\n      this.getUsersByCompId(company.id);\r\n      this.getClients(company.id);\r\n    },\r\n\r\n    getUsersByCompId(currentId) {\r\n      companyService.getUsersByCompanyId(currentId).then(\r\n        (response) => {\r\n          this.users = response.data;\r\n        },\r\n        (error) => {\r\n          this.users =\r\n            (error.response &&\r\n              error.response.data &&\r\n              error.response.data.message) ||\r\n            error.message ||\r\n            error.toString();\r\n        }\r\n      );\r\n      this.users = this.users.filter((v) => v.id !== this.currentUser.id);\r\n      console.log(this.users);\r\n    },\r\n\r\n    getClients(currentId) {\r\n      companyService.getClientsByCompany(currentId).then(\r\n        (response) => {\r\n          this.clients = response.data;\r\n        },\r\n        (error) => {\r\n          this.clients =\r\n            (error.response &&\r\n              error.response.data &&\r\n              error.response.data.message) ||\r\n            error.message ||\r\n            error.toString();\r\n        }\r\n      );\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    this.getAllCompanies();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.list {\r\n  text-align: left;\r\n  margin: auto;\r\n}\r\n.companyItem {\r\n  border: 2px solid teal;\r\n  border-radius: 5px;\r\n}\r\n.lists {\r\n  display: flex;\r\n}\r\n.usList{\r\n  margin-top: 0px;\r\n}\r\n</style>"],"mappings":"AA4CA,OAAOA,cAAa,MAAO,4BAA4B;AACvD,OAAOC,QAAO,MAAO,iBAAiB;AACtC,OAAOC,UAAS,MAAO,mBAAmB;AAC1C,OAAOC,WAAU,MAAO,yBAAyB;AAEjD,eAAe;EACbC,UAAU,EAAE;IAAEH,QAAQ;IAAEC;EAAW,CAAC;EACpC;EACAG,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,cAAc,EAAE,IAAI;MACpBC,SAAS,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChBV,WAAW,CAACW,kBAAkB,CAAC,IAAI,CAACC,WAAW,CAACC,EAAE,CAAC,CAACC,IAAI,CACrDC,QAAQ,IAAK;QACZ,IAAI,CAACX,SAAQ,GAAIW,QAAQ,CAACZ,IAAI;QAC9B,IAAGY,QAAQ,CAACC,MAAK,IAAK,GAAG,EAAC;UACxB,IAAI,CAACZ,SAAQ,GAAI,EAAE;QACrB;MACF,CAAC,EACAa,KAAK,IAAK;QACTC,OAAO,CAACC,GAAG,CAAC,CAAC;MACf,CACF,CAAC;IAEH,CAAC;IAEDC,gBAAgBA,CAACC,OAAO,EAAEf,SAAS,EAAE;MACnC,IAAI,CAACD,cAAa,GAAIgB,OAAO;MAC7B,IAAI,CAACf,SAAQ,GAAIe,OAAM,GAAIf,SAAQ,GAAI,CAAC,CAAC;MACzC,IAAI,CAACgB,gBAAgB,CAACD,OAAO,CAACR,EAAE,CAAC;MACjC,IAAI,CAACU,UAAU,CAACF,OAAO,CAACR,EAAE,CAAC;IAC7B,CAAC;IAEDS,gBAAgBA,CAAChB,SAAS,EAAE;MAC1BT,cAAc,CAAC2B,mBAAmB,CAAClB,SAAS,CAAC,CAACQ,IAAI,CAC/CC,QAAQ,IAAK;QACZ,IAAI,CAACR,KAAI,GAAIQ,QAAQ,CAACZ,IAAI;MAC5B,CAAC,EACAc,KAAK,IAAK;QACT,IAAI,CAACV,KAAI,GACNU,KAAK,CAACF,QAAO,IACZE,KAAK,CAACF,QAAQ,CAACZ,IAAG,IAClBc,KAAK,CAACF,QAAQ,CAACZ,IAAI,CAACsB,OAAO,IAC7BR,KAAK,CAACQ,OAAM,IACZR,KAAK,CAACS,QAAQ,CAAC,CAAC;MACp