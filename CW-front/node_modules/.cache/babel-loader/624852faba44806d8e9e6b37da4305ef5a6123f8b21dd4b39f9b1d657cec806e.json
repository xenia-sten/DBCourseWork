{"ast":null,"code":"import MyButton from \"./UI/MyButton.vue\";\nimport ClientService from \"../services/client.service\";\nimport TemplateService from \"../services/template.service\";\nimport { format } from \"../../node_modules/@formkit/tempo\";\nimport userService from \"@/services/user.service\";\nexport default {\n  components: {\n    MyButton\n  },\n  props: {\n    visit: {\n      type: Object,\n      required: true\n    }\n  },\n  data() {\n    return {\n      clientName: \"\",\n      templateName: \"\",\n      visitDate: \"\",\n      createDate: \"\",\n      userMaster: ''\n    };\n  },\n  mounted() {\n    this.visitDate = format({\n      date: this.visit.visit_date,\n      format: \"HH:mm DD-MM-YYYY\",\n      tz: \"Europe/Samara\"\n    });\n    this.createDate = format({\n      date: this.visit.creation_date,\n      format: \"HH:mm DD-MM-YYYY\",\n      tz: \"Europe/Samara\"\n    });\n    ClientService.getClient(this.visit.client_id == null ? this.visit.client_old_id : this.visit.client_id).then(response => {\n      this.clientName = response.data.name;\n      console.log(this.clientName);\n    }, error => {\n      this.client = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n    TemplateService.getTemplate(this.visit.template_id || this.visit.template_old_id).then(response => {\n      this.templateName = response.data.name;\n    }, error => {\n      this.templateName = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n    userService.getUserById(this.visit.user_id || this.visit.user_old_id).then(response => {\n      this.userMaster = response.data.name;\n    }, error => {\n      this.userName = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n  }\n};","map":{"version":3,"names":["MyButton","ClientService","TemplateService","format","userService","components","props","visit","type","Object","required","data","clientName","templateName","visitDate","createDate","userMaster","mounted","date","visit_date","tz","creation_date","getClient","client_id","client_old_id","then","response","name","console","log","error","client","message","toString","getTemplate","template_id","template_old_id","getUserById","user_id","user_old_id","userName"],"sources":["C:\\Users\\Admin\\Desktop\\X\\SSTU\\6 семак\\БД\\DBCourseWork\\CW-front\\src\\components\\VisitItem.vue"],"sourcesContent":["<template>\r\n  <div class=\"visit\">\r\n    <div>\r\n      <div><strong>Мастер:</strong> {{ userMaster }}</div>\r\n      <div><strong>Дата:</strong> {{ visitDate }}</div>\r\n      <div><strong>Клиент:</strong> {{ clientName }}</div>\r\n      <div><strong>Услуга:</strong> {{ templateName }}</div>\r\n      <div><strong>Описание:</strong> {{ visit.description }}</div>\r\n      <div><strong>Дата создания:</strong> {{ createDate }}</div>\r\n    </div>\r\n    <div class=\"visit__btns\">\r\n      <!-- <my-button @click=\"$router.push(`/posts/${post.id}`)\">\r\n        Открыть\r\n      </my-button> -->\r\n      <my-button @click=\"$emit('remove', visit)\"> Удалить </my-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MyButton from \"./UI/MyButton.vue\";\r\nimport ClientService from \"../services/client.service\";\r\nimport TemplateService from \"../services/template.service\";\r\nimport { format } from \"../../node_modules/@formkit/tempo\";\r\nimport userService from \"@/services/user.service\";\r\n\r\nexport default {\r\n  components: { MyButton },\r\n  props: {\r\n    visit: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      clientName: \"\",\r\n      templateName: \"\",\r\n      visitDate: \"\",\r\n      createDate: \"\",\r\n      userMaster: '',\r\n    };\r\n  },\r\n  mounted() {\r\n    this.visitDate = format({\r\n      date: this.visit.visit_date,\r\n      format: \"HH:mm DD-MM-YYYY\",\r\n      tz: \"Europe/Samara\",\r\n    });\r\n    this.createDate = format({\r\n      date: this.visit.creation_date,\r\n      format: \"HH:mm DD-MM-YYYY\",\r\n      tz: \"Europe/Samara\",\r\n    });\r\n    ClientService.getClient(this.visit.client_id == null ? this.visit.client_old_id : this.visit.client_id).then(\r\n      (response) => {\r\n        this.clientName = response.data.name;\r\n        console.log(this.clientName);\r\n      },\r\n      (error) => {\r\n        this.client =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n    TemplateService.getTemplate(this.visit.template_id  || this.visit.template_old_id).then(\r\n      (response) => {\r\n        this.templateName = response.data.name;\r\n      },\r\n      (error) => {\r\n        this.templateName =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n    userService.getUserById(this.visit.user_id || this.visit.user_old_id).then(\r\n      (response) => {\r\n        this.userMaster = response.data.name;\r\n      },\r\n      (error) => {\r\n        this.userName =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.visit {\r\n  padding: 15px;\r\n  border: 2px solid teal;\r\n  border-radius: 5px;\r\n  margin-top: 15px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n.visit__btns {\r\n  display: flex;\r\n}\r\n</style>"],"mappings":"AAoBA,OAAOA,QAAO,MAAO,mBAAmB;AACxC,OAAOC,aAAY,MAAO,4BAA4B;AACtD,OAAOC,eAAc,MAAO,8BAA8B;AAC1D,SAASC,MAAK,QAAS,mCAAmC;AAC1D,OAAOC,WA