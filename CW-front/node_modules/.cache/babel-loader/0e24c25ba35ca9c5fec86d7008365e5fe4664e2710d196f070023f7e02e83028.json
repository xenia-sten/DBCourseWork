{"ast":null,"code":"import companyService from \"@/services/company.service\";\nimport UserList from \"../UserList.vue\";\nimport ClientList from \"../ClientList.vue\";\nimport userService from \"@/services/user.service\";\nimport MyButton from \"../UI/MyButton.vue\";\nimport MyDialog from \"../UI/MyDialog.vue\";\nimport CompanyForm from \"../CompanyForm.vue\";\nexport default {\n  components: {\n    UserList,\n    ClientList,\n    MyButton,\n    MyDialog,\n    CompanyForm\n  },\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: \"Companies\",\n  data() {\n    return {\n      companies: [],\n      currentCompany: null,\n      currentId: -1,\n      users: [],\n      clients: [],\n      dialogVisible: false,\n      addedCompany: \"\"\n    };\n  },\n  methods: {\n    getAllCompanies() {\n      userService.getCompaniesByUser(this.currentUser.id).then(response => {\n        this.companies = response.data;\n        if (response.status == 404) {\n          this.companies = [];\n        }\n      }, error => {\n        console.log(error);\n      });\n    },\n    setActiveCompany(company, currentId) {\n      this.currentCompany = company;\n      this.currentId = company ? currentId : -1;\n      this.getUsersByCompId(company.id);\n      this.getClients(company.id);\n    },\n    getUsersByCompId(currentId) {\n      companyService.getUsersByCompanyId(currentId).then(response => {\n        this.users = response.data;\n      }, error => {\n        this.users = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n      });\n    },\n    getClients(currentId) {\n      companyService.getClientsByCompany(currentId).then(response => {\n        this.clients = response.data;\n        if (response.status == 404) {\n          this.clients = [];\n        }\n      }, error => {\n        console.log(error);\n      });\n    },\n    showDialog() {\n      this.dialogVisible = true;\n    },\n    createCompany(company) {\n      var data = {\n        name: company.name,\n        description: company.description\n      };\n      companyService.createCompany(data).then(response => {\n        console.log(response.data);\n        this.bindUserWithCompany(this.currentUser.id, response.data.id);\n      }).catch(error => console.log(error));\n      this.dialogVisible = false;\n    },\n    bindUserWithCompany(userId, companyId) {\n      companyService.addUserInCompany(userId, companyId).then(response => {\n        console.log(response);\n      }).catch(e => console.log(e));\n    }\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  mounted() {\n    this.getAllCompanies();\n  }\n};","map":{"version":3,"names":["companyService","UserList","ClientList","userService","MyButton","MyDialog","CompanyForm","components","name","data","companies","currentCompany","currentId","users","clients","dialogVisible","addedCompany","methods","getAllCompanies","getCompaniesByUser","currentUser","id","then","response","status","error","console","log","setActiveCompany","company","getUsersByCompId","getClients","getUsersByCompanyId","message","toString","getClientsByCompany","showDialog","createCompany","description","bindUserWithCompany","catch","userId","companyId","addUserInCompany","e","computed","$store","state","auth","user","mounted"],"sources":["C:\\Users\\Admin\\Desktop\\X\\SSTU\\6 семак\\БД\\DBCourseWork\\CW-front\\src\\components\\pages\\CompaniesPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"list row\">\r\n    <div class=\"col-md-3\">\r\n      <br />\r\n      <h4>Список компаний:</h4>\r\n      <my-button id=\"btn-add-company\" @click=\"showDialog\"> Создать </my-button>\r\n      <my-dialog :show=\"dialogVisible\">\r\n        <company-form @create=\"createCompany\" />\r\n      </my-dialog>\r\n      <ul class=\"list-group\" v-if=\"this.companies.length > 0\">\r\n        <li\r\n          class=\"companyItem list-group-item\"\r\n          :class=\"{ active: id == currentId }\"\r\n          v-for=\"(company, id) in companies\"\r\n          :key=\"company.id\"\r\n          @click=\"setActiveCompany(company, currentId)\"\r\n        >\r\n          {{ company.name }}\r\n        </li>\r\n      </ul>\r\n      <h5 v-else style=\"color: red\">Список компаний пуст</h5>\r\n    </div>\r\n    <div class=\"col-md-9\">\r\n      <br />\r\n      <div v-if=\"currentCompany\">\r\n        <h4>Выбранная компания</h4>\r\n        <div>\r\n          <label><strong>Name: </strong></label> {{ currentCompany.name }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Description: </strong></label>\r\n          {{ currentCompany.description }}\r\n        </div>\r\n        <div class=\"container lists\">\r\n          <user-list\r\n            class=\"usList\"\r\n            :users=\"users\"\r\n            @remove=\"removeUser\"\r\n          ></user-list>\r\n          <client-list :clients=\"clients\"></client-list>\r\n        </div>\r\n        <!-- <router-link :to=\"'/tutorials/' + currentTutorial.id\" class=\"badge badge-warning\">Edit</router-link> -->\r\n      </div>\r\n      <div v-else>\r\n        <br />\r\n        <p>Please click on a Company...</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport companyService from \"@/services/company.service\";\r\nimport UserList from \"../UserList.vue\";\r\nimport ClientList from \"../ClientList.vue\";\r\nimport userService from \"@/services/user.service\";\r\nimport MyButton from \"../UI/MyButton.vue\";\r\nimport MyDialog from \"../UI/MyDialog.vue\";\r\nimport CompanyForm from \"../CompanyForm.vue\";\r\n\r\nexport default {\r\n  components: { UserList, ClientList, MyButton, MyDialog, CompanyForm },\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: \"Companies\",\r\n  data() {\r\n    return {\r\n      companies: [],\r\n      currentCompany: null,\r\n      currentId: -1,\r\n      users: [],\r\n      clients: [],\r\n      dialogVisible: false,\r\n      addedCompany: \"\",\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    getAllCompanies() {\r\n      userService.getCompaniesByUser(this.currentUser.id).then(\r\n        (response) => {\r\n          this.companies = response.data;\r\n          if (response.status == 404) {\r\n            this.companies = [];\r\n          }\r\n        },\r\n        (error) => {\r\n          console.log(error);\r\n        }\r\n      );\r\n    },\r\n\r\n    setActiveCompany(company, currentId) {\r\n      this.currentCompany = company;\r\n      this.currentId = company ? currentId : -1;\r\n      this.getUsersByCompId(company.id);\r\n      this.getClients(company.id);\r\n    },\r\n\r\n    getUsersByCompId(currentId) {\r\n      companyService.getUsersByCompanyId(currentId).then(\r\n        (response) => {\r\n          this.users = response.data;\r\n        },\r\n        (error) => {\r\n          this.users =\r\n            (error.response &&\r\n              error.response.data &&\r\n              error.response.data.message) ||\r\n            error.message ||\r\n            error.toString();\r\n        }\r\n      );\r\n    },\r\n\r\n    getClients(currentId) {\r\n      companyService.getClientsByCompany(currentId).then(\r\n        (response) => {\r\n          this.clients = response.data;\r\n          if (response.status == 404) {\r\n            this.clients = [];\r\n          }\r\n        },\r\n        (error) => {\r\n          console.log(error);\r\n        }\r\n      );\r\n    },\r\n\r\n    showDialog() {\r\n      this.dialogVisible = true;\r\n    },\r\n\r\n    createCompany(company) {\r\n      var data = {\r\n        name: company.name,\r\n        description: company.description,\r\n      };\r\n      companyService\r\n        .createCompany(data)\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.bindUserWithCompany(this.currentUser.id, response.data.id);\r\n        })\r\n        .catch((error) => console.log(error));\r\n      this.dialogVisible = false;\r\n    },\r\n\r\n    bindUserWithCompany(userId, companyId) {\r\n      companyService\r\n        .addUserInCompany(userId, companyId)\r\n        .then((response) => {\r\n          console.log(response);\r\n        })\r\n        .catch((e) => console.log(e));\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    this.getAllCompanies();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.list {\r\n  text-align: left;\r\n  margin: auto;\r\n}\r\n.companyItem {\r\n  border: 2px solid teal;\r\n  border-radius: 5px;\r\n}\r\n.lists {\r\n  display: flex;\r\n}\r\n.usList {\r\n  margin-top: 0px;\r\n}\r\n</style>"],"mappings":"AAoDA,OAAOA,cAAa,MAAO,4BAA4B;AACvD,OAAOC,QAAO,MAAO,iBAAiB;AACtC,OAAOC,UAAS,MAAO,mBAAmB;AAC1C,OAAOC,WAAU,MAAO,yBAAyB;AACjD,OAAOC,QAAO,MAAO,oBAAoB;AACzC,OAAOC,QAAO,MAAO,oBAAoB;AACzC,OAAOC,WAAU,MAAO,oBAAoB;AAE5C,eAAe;EACbC,UAAU,EAAE;IAAEN,QAAQ;IAAEC,UAAU;IAAEE,QAAQ;IAAEC,QAAQ;IAAEC;EAAY,CAAC;EACrE;EACAE,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,cAAc,EAAE,IAAI;MACpBC,SAAS,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,aAAa,EAAE,KAAK;MACpBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChBf,WAAW,CAACgB,kBAAkB,CAAC,IAAI,CAACC,WAAW,CAACC,EAAE,CAAC,CAACC,IAAI,CACrDC,QAAQ,IAAK;QACZ,IAAI,CAACb,SAAQ,GAAIa,QAAQ,CAACd,IAAI;QAC9B,IAAIc,QAAQ,CAACC,MAAK,IAAK,GAAG,EAAE;UAC1B,IAAI,CAACd,SAAQ,GAAI,EAAE;QACrB;MACF,CAAC,EACAe,KAAK,IAAK;QACTC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CACF,CAAC;IACH,CAAC;IAEDG,gBAAgBA,CAACC,OAAO,EAAEjB,SAAS,EAAE;MACnC,IAAI,CAACD,cAAa,GAAIkB,OAAO;MAC7B,IAAI,CAACjB,SAAQ,GAAIiB,OAAM,GAAIjB,SAAQ,GAAI,CAAC,CAAC;MACzC,IAAI,CAACkB,gBAAgB,CAACD,OAAO,CAACR,EAAE,CAAC;MACjC,IAAI,CAACU,UAAU,CAACF,OAAO,CAACR,EAAE,CAAC;IAC7B,CAAC;IAEDS,gBAAgBA,CAAClB,SAAS,EAAE;MAC1BZ,cAAc,CAACgC,mBAAmB,CAACpB,SAAS,CAAC,CAACU,IAAI,CAC/CC,QAAQ,IAAK;QACZ,IAAI,CAACV,KAAI,GAAIU,Q