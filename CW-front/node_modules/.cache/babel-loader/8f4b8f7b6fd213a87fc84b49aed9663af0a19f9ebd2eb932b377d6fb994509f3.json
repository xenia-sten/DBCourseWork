{"ast":null,"code":"import MyButton from './UI/MyButton.vue';\nimport ClientService from \"../services/client.service\";\nimport TemplateService from \"../services/template.service\";\nimport { format } from \"../../node_m\";\nexport default {\n  components: {\n    MyButton\n  },\n  props: {\n    visit: {\n      type: Object,\n      required: true\n    }\n  },\n  data() {\n    return {\n      clientName: '',\n      templateName: '',\n      visitDate: ''\n    };\n  },\n  methods: {\n    convertVisitDate() {\n      this.visitDate = format({\n        date: this.visit.visit_date,\n        format: \"HH:mm DD-MM-YYYY\",\n        tz: \"Russia/Samara\"\n      });\n    }\n  },\n  mounted() {\n    ClientService.getClient(this.visit.client_id).then(response => {\n      this.clientName = response.data.name;\n    }, error => {\n      this.client = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n    TemplateService.getTemplate(this.visit.template_id).then(response => {\n      this.templateName = response.data.name;\n    }, error => {\n      this.templateName = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n  }\n};","map":{"version":3,"names":["MyButton","ClientService","TemplateService","format","components","props","visit","type","Object","required","data","clientName","templateName","visitDate","methods","convertVisitDate","date","visit_date","tz","mounted","getClient","client_id","then","response","name","error","client","message","toString","getTemplate","template_id"],"sources":["C:\\Users\\Admin\\Desktop\\X\\SSTU\\6 семак\\БД\\DBCourseWork\\CW-front\\src\\components\\VisitItem.vue"],"sourcesContent":["<template>\r\n  <div class=\"visit\">\r\n    <div>\r\n      <div>{{ visit.id }}</div>\r\n      <div><strong>Дата:</strong> {{ visitDate }}</div>\r\n      <div><strong>Клиент:</strong> {{ clientName }}</div>\r\n      <div><strong>Услуга:</strong> {{ templateName }}</div>\r\n      <div><strong>Описание:</strong> {{ visit.description }}</div>\r\n      <div><strong>Дата создания:</strong> {{ visit.creation_date }}</div>\r\n    </div>\r\n    <div class=\"visit__btns\">\r\n      <!-- <my-button @click=\"$router.push(`/posts/${post.id}`)\">\r\n        Открыть\r\n      </my-button> -->\r\n      <my-button @click=\"$emit('remove', visit)\"> Удалить </my-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MyButton from './UI/MyButton.vue';\r\nimport ClientService from \"../services/client.service\";\r\nimport TemplateService from \"../services/template.service\";\r\nimport {format} from \"../../node_m\"\r\n\r\nexport default {\r\n  components: { MyButton },\r\n  props: {\r\n    visit: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      clientName: '',\r\n      templateName: '',\r\n      visitDate: '',\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    convertVisitDate(){\r\n      this.visitDate = format({\r\n        date: this.visit.visit_date,\r\n        format: \"HH:mm DD-MM-YYYY\",\r\n        tz: \"Russia/Samara\"\r\n      });\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    ClientService.getClient(this.visit.client_id).then(\r\n      (response) => {\r\n        this.clientName = response.data.name;\r\n      },\r\n      (error) => {\r\n        this.client =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n    TemplateService.getTemplate(this.visit.template_id).then(\r\n      (response) => {\r\n        this.templateName = response.data.name;\r\n      },\r\n      (error) => {\r\n        this.templateName =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.visit {\r\n  padding: 15px;\r\n  border: 2px solid teal;\r\n  border-radius: 5px;\r\n  margin-top: 15px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n.visit__btns {\r\n  display: flex;\r\n}\r\n</style>"],"mappings":"AAoBA,OAAOA,QAAO,MAAO,mBAAmB;AACxC,OAAOC,aAAY,MAAO,4BAA4B;AACtD,OAAOC,eAAc,MAAO,8BAA8B;AAC1D,SAAQC,MAAM,QAAO,cAAa;AAElC,eAAe;EACbC,UAAU,EAAE;IAAEJ;EAAS,CAAC;EACxBK,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE;IACb;EACF,CAAC;EAEDC,OAAO,EAAE;IACPC,gBAAgBA,CAAA,EAAE;MAChB,IAAI,CAACF,SAAQ,GAAIV,MAAM,CAAC;QACtBa,IAAI,EAAE,IAAI,CAACV,KAAK,CAACW,UAAU;QAC3Bd,MAAM,EAAE,kBAAkB;QAC1Be,EAAE,EAAE;MACN,CAAC,CAAC;IACJ;EACF,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACRlB,aAAa,CAACmB,SAAS,CAAC,IAAI,CAACd,KAAK,CAACe,SAAS,CAAC,CAACC,IAAI,CAC/CC,QAAQ,IAAK;MACZ,IAAI,CAACZ,UAAS,GAAIY,QAAQ,CAACb,IAAI,CAACc,IAAI;IACtC,CAAC,EACAC,KAAK,IAAK;MACT,IAAI,CAACC,MAAK,GACPD,KAAK,CAACF,QAAO,IACZE,KAAK,CAACF,QAAQ,CAACb,IAAG,IAClBe,KAAK,CAACF,QAAQ,CAACb,IAAI,CAACiB,OAAO,IAC7BF,KAAK,CAACE,OAAM,IACZF,KAAK,CAACG,QAAQ,CAAC,CAAC;IACpB,CACF,CAAC;IACD1B,eAAe,CAAC2B,WAAW,CAAC,IAAI,CAACvB,KAAK,CAACwB,WAAW,CAAC,CAACR,IAAI,CACrDC,QAAQ,IAAK;MACZ,IAAI,CAACX,YAAW,GAAIW,QAAQ,CAACb,IAAI,CAACc,IAAI;IACxC,CAAC,EACAC,KAAK,IAAK;MACT,IAAI,CAACb,YAAW,GACba,KAAK,CAACF,QAAO,IACZE,KAAK,CAACF,QAAQ,CAACb,IAAG,IAClBe,KAAK,CAACF,QAAQ,CAACb,IAAI,CAACiB,OAAO,IAC7BF,KAAK,CAACE,OAAM,IACZF,KAAK,CAACG,QAAQ,CAAC,CAAC;IACpB,CACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}