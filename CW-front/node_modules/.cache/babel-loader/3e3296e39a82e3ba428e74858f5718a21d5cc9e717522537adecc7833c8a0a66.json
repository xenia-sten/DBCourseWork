nt_id: visit.client_id,\r\n          visit_date: visit.visit_date,\r\n          description: visit.description,\r\n        })\r\n        .then(\r\n          (response) => {\r\n            console.log(response);\r\n          },\r\n          (error) => {\r\n            this.visits =\r\n              (error.response &&\r\n                error.response.data &&\r\n                error.response.data.message) ||\r\n              error.message ||\r\n              error.toString();\r\n          }\r\n        );\r\n      this.dialogVisible = false;\r\n    },\r\n    removeVisit(visit) {\r\n      visitService.deleteVisit(visit.id).then(\r\n        (response) => {\r\n          console.log(response);\r\n        },\r\n        (error) => {\r\n          visit =\r\n            (error.response &&\r\n              error.response.data &&\r\n              error.response.data.message) ||\r\n            error.message ||\r\n            error.toString();\r\n        }\r\n      );\r\n    },\r\n    loadVisits(){\r\n      UserService.getVisitsByUser(this.currentUser.id).then(\r\n      (response) => {\r\n        this.visits = response.data;\r\n      },\r\n      (error) => {\r\n        this.visits =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n    },\r\n    showDialog() {\r\n      this.dialogVisible = true;\r\n    },\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.loadVisits()\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.visits__btns {\r\n  margin: 15px 0;\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n</style>"],"mappings":"AAiBA,OAAOA,WAAU,MAAO,0BAA0B;AAClD,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,YAAW,MAAO,0BAA0B;AAEnD,eAAe;EACbC,UAAU,EAAE;IAAEL,SA