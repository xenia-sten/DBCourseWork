{"ast":null,"code":"import UserService from \"../services/user.service\";\nimport VisitList from \"./VisitList.vue\";\nimport MyDialog from \"./UI/MyDialog.vue\";\nimport VisitForm from \"./VisitForm.vue\";\nimport MyButton from \"./UI/MyButton.vue\";\nimport visitService from \"@/services/visit.service\";\nexport default {\n  components: {\n    VisitList,\n    VisitForm,\n    MyDialog,\n    MyButton\n  },\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: \"Visits\",\n  data() {\n    return {\n      visits: [],\n      isVisitsLoading: false,\n      dialogVisible: false,\n      delVisit: \"\",\n      renderComponent: true\n    };\n  },\n  methods: {\n    forceRerender() {\n      // Сначала скроем компонент\n      this.renderComponent = false;\n      this.$nextTick(() => {\n        // А потом покажем снова\n        this.renderComponent = true;\n      });\n    },\n    createVisit(visit) {\n      visitService.createVisit({\n        company_id: visit.company_id,\n        user_id: visit.user_id,\n        template_id: visit.template_id,\n        client_id: visit.client_id,\n        visit_date: visit.visit_date,\n        description: visit.description\n      }).then(response => {\n        console.log(response);\n      }, error => {\n        this.visits = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n      });\n      this.forceRerender();\n      this.dialogVisible = false;\n    },\n    removeVisit(visit) {\n      this.visits = this.visits.filter(v => v.id !== visit.id);\n    },\n    showDialog() {\n      this.dialogVisible = true;\n    }\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  mounted() {\n    UserService.getVisitsByUser(this.currentUser.id).then(response => {\n      this.visits = response.data;\n    }, error => {\n      this.visits = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n  }\n};","map":{"version":3,"names":["UserService","VisitList","MyDialog","VisitForm","MyButton","visitService","components","name","data","visits","isVisitsLoading","dialogVisible","delVisit","renderComponent","methods","forceRerender","$nextTick","createVisit","visit","company_id","user_id","template_id","client_id","visit_date","description","then","response","console","log","error","message","toString","removeVisit","filter","v","id","showDialog","computed","currentUser","$store","state","auth","user","mounted","getVisitsByUser"],"sources":["C:\\Users\\Admin\\Desktop\\X\\SSTU\\6 семак\\БД\\DBCourseWork\\CW-front\\src\\components\\VisitsPage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Страница с визитами</h1>\r\n    <div class=\"visits__btns\">\r\n      <my-button @click=\"showDialog\"> Создать визит </my-button>\r\n    </div>\r\n    <my-dialog :show=\"dialogVisible\">\r\n      <visit-form @create=\"createVisit\" />\r\n    </my-dialog>\r\n    <div class=\"container\" v-if=\"renderComponent\">\r\n      <visit-list :visits=\"visits\" @remove=\"removeVisit\" />\r\n      <!-- <div v-else>Идет загрузка...</div> -->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserService from \"../services/user.service\";\r\nimport VisitList from \"./VisitList.vue\";\r\nimport MyDialog from \"./UI/MyDialog.vue\";\r\nimport VisitForm from \"./VisitForm.vue\";\r\nimport MyButton from \"./UI/MyButton.vue\";\r\nimport visitService from \"@/services/visit.service\";\r\n\r\nexport default {\r\n  components: { VisitList, VisitForm, MyDialog, MyButton },\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: \"Visits\",\r\n  data() {\r\n    return {\r\n      visits: [],\r\n      isVisitsLoading: false,\r\n      dialogVisible: false,\r\n      delVisit: \"\",\r\n      renderComponent: true,\r\n    };\r\n  },\r\n  methods: {\r\n    forceRerender() {\r\n      // Сначала скроем компонент\r\n      this.renderComponent = false;\r\n\r\n      this.$nextTick(() => {\r\n        // А потом покажем снова\r\n        this.renderComponent = true;\r\n      });\r\n    },\r\n\r\n    createVisit(visit) {\r\n      visitService.createVisit({company_id: visit.company_id, user_id: visit.user_id, template_id: visit.template_id, client_id: visit.client_id, visit_date: visit.visit_date, description: visit.description}).then(\r\n        (response) => {\r\n          console.log(response);\r\n        },\r\n        (error) => {\r\n          this.visits =\r\n            (error.response &&\r\n              error.response.data &&\r\n              error.response.data.message) ||\r\n            error.message ||\r\n            error.toString();\r\n        }\r\n      );\r\n      this.forceRerender();\r\n      this.dialogVisible = false;\r\n    },\r\n    removeVisit(visit) {\r\n      this.visits = this.visits.filter((v) => v.id !== visit.id);\r\n    },\r\n    showDialog() {\r\n      this.dialogVisible = true;\r\n    },\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  mounted() {\r\n    UserService.getVisitsByUser(this.currentUser.id).then(\r\n      (response) => {\r\n        this.visits = response.data;\r\n      },\r\n      (error) => {\r\n        this.visits =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.visits__btns {\r\n  margin: 15px 0;\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n</style>"],"mappings":"AAiBA,OAAOA,WAAU,MAAO,0BAA0B;AAClD,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,YAAW,MAAO,0BAA0B;AAEnD,eAAe;EACbC,UAAU,EAAE;IAAEL,SAAS;IAAEE,SAAS;IAAED,QAAQ;IAAEE;EAAS,CAAC;EACxD;EACAG,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACV