{"ast":null,"code":"import clientService from \"@/services/client.service\";\nimport ClientForm from \"./ClientForm.vue\";\nimport MyButton from \"./UI/MyButton.vue\";\nimport MyDialog from \"./UI/MyDialog.vue\";\nimport VisitList from './VisitList.vue';\nexport default {\n  components: {\n    MyButton,\n    MyDialog,\n    ClientForm,\n    VisitList\n  },\n  props: {\n    clients: {\n      type: Array,\n      required: true\n    }\n  },\n  data() {\n    return {\n      dialogVisible: false,\n      historyVisible: false,\n      visitList: []\n    };\n  },\n  methods: {\n    showDialog() {\n      this.dialogVisible = true;\n    },\n    showHistory(client) {\n      this.historyVisible = true;\n      clientService.getVisitsByClientId(client.id).then(response => {\n        this.visitList = response.data;\n      }).catch(e => console.log(e));\n    },\n    createClient(client) {\n      var data = {\n        company_id: client.company_id,\n        name: client.name,\n        email: client.email,\n        phone_number: client.phone_number,\n        description: client.descripiton\n      };\n      clientService.createClient(data).then(response => {\n        console.log(response);\n      }).catch(e => console.log(e));\n      this.dialogVisible = false;\n    }\n  }\n};","map":{"version":3,"names":["clientService","ClientForm","MyButton","MyDialog","VisitList","components","props","clients","type","Array","required","data","dialogVisible","historyVisible","visitList","methods","showDialog","showHistory","client","getVisitsByClientId","id","then","response","catch","e","console","log","createClient","company_id","name","email","phone_number","description","descripiton"],"sources":["C:\\Users\\Admin\\Desktop\\X\\SSTU\\6 семак\\БД\\DBCourseWork\\CW-front\\src\\components\\ClientList.vue"],"sourcesContent":["<template>\r\n  <div class=\"list row\">\r\n    <div>\r\n      <h5>Список клиентов:</h5>\r\n      <my-button id=\"btn-add-client\" @click=\"showDialog\"> Создать </my-button>\r\n      <my-dialog v-model:show=\"dialogVisible\">\r\n        <client-form @create=\"createClient\" />\r\n      </my-dialog>\r\n      <ul class=\"list-group\" v-if=\"this.clients.length > 0\">\r\n        <li\r\n          class=\"clientItem list-group-item\"\r\n          v-for=\"client in clients\"\r\n          :key=\"client.id\"\r\n        >\r\n          {{ client.name }}\r\n          <button class=\"badge badge-success\" @click=\"showHistory(client)\">\r\n            History\r\n          </button>\r\n          <!-- eslint-disable -->\r\n          <my-dialog v-model:show=\"historyVisible\" class=\"client_dialog\">\r\n            <visit-list :visits=\"visitList\"/>\r\n          </my-dialog>\r\n          <router-link :to=\"'/clients/' + client.id\" class=\"badge badge-warning\"\r\n            >Edit</router-link\r\n          >\r\n        </li>\r\n      </ul>\r\n      <h5 v-else style=\"color: red\">Список клиентов пуст</h5>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport clientService from \"@/services/client.service\";\r\nimport ClientForm from \"./ClientForm.vue\";\r\nimport MyButton from \"./UI/MyButton.vue\";\r\nimport MyDialog from \"./UI/MyDialog.vue\";\r\nimport VisitList from './VisitList.vue';\r\n\r\nexport default {\r\n  components: { MyButton, MyDialog, ClientForm, VisitList },\r\n  props: {\r\n    clients: {\r\n      type: Array,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      historyVisible: false,\r\n      visitList: [],\r\n    };\r\n  },\r\n  methods: {\r\n    showDialog() {\r\n      this.dialogVisible = true;\r\n    },\r\n    showHistory(client){\r\n      this.historyVisible = true;\r\n      clientService.getVisitsByClientId(client.id).then(\r\n        (response) => {\r\n          this.visitList = response.data;\r\n        }\r\n      ).catch(e => console.log(e));\r\n    },\r\n    createClient(client) {\r\n      var data = {\r\n        company_id: client.company_id,\r\n        name: client.name,\r\n        email: client.email,\r\n        phone_number: client.phone_number,\r\n        description: client.descripiton,\r\n      };\r\n      clientService\r\n        .createClient(data)\r\n        .then((response) => {\r\n          console.log(response);\r\n        })\r\n        .catch((e) => console.log(e));\r\n      this.dialogVisible = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.clientItem {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  border: 2px solid teal;\r\n  border-radius: 5px;\r\n  min-width: 300px;\r\n  position: static;\r\n}\r\n#btn-add-client {\r\n  width: 200px;\r\n  margin-bottom: 10px;\r\n}\r\n.client_dialog{\r\n  overflow-y: scroll;\r\n}\r\n</style>"],"mappings":"AAiCA,OAAOA,aAAY,MAAO,2BAA2B;AACrD,OAAOC,UAAS,MAAO,kBAAkB;AACzC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,SAAQ,MAAO,iBAAiB;AAEvC,eAAe;EACbC,UAAU,EAAE;IAAEH,QAAQ;IAAEC,QAAQ;IAAEF,UAAU;IAAEG;EAAU,CAAC;EACzDE,KAAK,EAAE;IACLC,OAAO,EAAE;MACPC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,KAAK;MACpBC,cAAc,EAAE,KAAK;MACrBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACJ,aAAY,GAAI,IAAI;IAC3B,CAAC;IACDK,WAAWA,CAACC,MAAM,EAAC;MACjB,IAAI,CAACL,cAAa,GAAI,IAAI;MAC1Bb,aAAa,CAACmB,mBAAmB,CAACD,MAAM,CAACE,EAAE,CAAC,CAACC,IAAI,CAC9CC,QAAQ,IAAK;QACZ,IAAI,CAACR,SAAQ,GAAIQ,QAAQ,CAACX,IAAI;MAChC,CACF,CAAC,CAACY,KAAK,CAACC,CAAA,IAAKC,OAAO,CAACC,GAAG,CAACF,CAAC,CAAC,CAAC;IAC9B,CAAC;IACDG,YAAYA,CAACT,MAAM,EAAE;MACnB,IAAIP,IAAG,GAAI;QACTiB,UAAU,EAAEV,MAAM,CAACU,UAAU;QAC7BC,IAAI,EAAEX,MAAM,CAACW,IAAI;QACjBC,KAAK,EAAEZ,MAAM,CAACY,KAAK;QACnBC,YAAY,EAAEb,MAAM,CAACa,YAAY;QACjCC,WAAW,EAAEd,MAAM,CAACe;MACtB,CAAC;MACDjC,aAAY,CACT2B,YAAY,CAAChB,IAAI,EACjBU,IAAI,CAAEC,QAAQ,IAAK;QAClBG,OAAO,CAACC,GAAG,CAACJ,QAAQ,CAAC;MACvB,CAAC,EACAC,KAAK,CAAEC,CAAC,IAAKC,OAAO,CAACC,GAAG,CAACF,CAAC,CAAC,CAAC;MAC/B,IAAI,CAACZ,aAAY,GAAI,KAAK;IAC5B;EACF;