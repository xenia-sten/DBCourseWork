   error.toString();\r\n        }\r\n      );\r\n      this.visits = this.visits.filter((v) => v.id !== visit.id);\r\n    },\r\n\r\n    showDialog() {\r\n      this.dialogVisible = true;\r\n    },\r\n  },\r\n  watch: {\r\n    selectedSort(newValue) {\r\n      this.visits.sort((visit1, visit2) => {\r\n        return visit1[newValue]?.localeCompare(visit2[newValue]);\r\n      });\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  mounted() {\r\n    UserService.getVisitsByUser(this.currentUser.id).then(\r\n      (response) => {\r\n        this.visits = response.data;\r\n      },\r\n      (error) => {\r\n        this.visits =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.visits__btns {\r\n  margin: 15px 0;\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n</style>"],"mappings":";AA2BA,OAAOA,WAAU,MAAO,0BAA0B;AAClD,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,YAAW,MAAO,0BAA0B;AAEnD,eAAe;EACbC,UAAU,EAAE;IAAEL,SAAS;IAAEE,SAAS;IAAED,QAAQ;IAAEE;EAAQ,CAAC;EACvD;EACAG,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,eAAe,EAAE,KAAK;MACtBC,aAAa,EAAE,KAAK;MACpBC,YAAY,EAAE,EAAE;MAChBC,WAAW,EAAE,CACX;QAAEC,KAAK,EAAE,WAAW;QAAEP,IAAI,EAAE;MAAoB,CAAC,EACjD;QAAEO,KAAK,EAAE,YAAY;QAAEP,IAAI,EAAE;MAAmB,CAAC;IAErD,CAAC;EACH,CAAC;EACDQ,OAAO,EAAE;IACPC,WAAWA,CAACC,KAAK,EAAE;MACjBZ,YAAW,CACRW,WAAW,CAAC;QACXE,UAAU,EAAED,KAAK,CAACC,UAAU;QAC5BC,OAAO,EAAEF,KAAK,CAACE,OAAO;QACtBC,WAAW,EAAEH,KAAK,CAACG,WAAW;QAC9BC,SAAS,EAAEJ,KAAK,CAACI,SAAS;QAC1BC,UAAU,EAAEL,KAAK,CAACK,UAAU;QAC5BC,WAAW,EAAEN,KAAK,CAACM;MACrB,CAAC,EACAC,IAAI,CACFC,QAAQ,IAAK;QACZC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;MACvB,CAAC,EACAG,KAAK,IAAK;QACT,