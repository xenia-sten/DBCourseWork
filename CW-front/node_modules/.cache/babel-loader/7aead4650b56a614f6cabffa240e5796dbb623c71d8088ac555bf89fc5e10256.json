{"ast":null,"code":"import UserService from \"../services/user.service\";\nimport VisitList from \"./VisitList.vue\";\nimport MyDialog from \"./UI/MyDialog.vue\";\nimport VisitForm from \"./VisitForm.vue\";\nimport MyButton from \"./UI/MyButton.vue\";\nimport visitService from \"@/services/visit.service\";\nexport default {\n  components: {\n    VisitList,\n    VisitForm,\n    MyDialog,\n    MyButton\n  },\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: \"Visits\",\n  data() {\n    return {\n      visits: [],\n      isVisitsLoading: false,\n      dialogVisible: false,\n      delVisit: \"\"\n    };\n  },\n  methods: {\n    createVisit(visit) {\n      //this.visits.push(visit);\n      console.log(JSON.stringify(visit));\n      //JSON.stringify(visit)\n      visitService.createVisit({\n        company_id: visit.company_id,\n        user_id: visit.user_id,\n        template_id: this.visit.template_id,\n        client_id: this.visit.client_id,\n        visit_date: this.visit.visit_date,\n        description: this.visit.description\n      }).then(response => {\n        console.log(response);\n      }, error => {\n        this.visits = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n      });\n      this.dialogVisible = false;\n    },\n    removeVisit(visit) {\n      this.visits = this.visits.filter(v => v.id !== visit.id);\n    },\n    showDialog() {\n      this.dialogVisible = true;\n    }\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  mounted() {\n    UserService.getVisitsByUser(this.currentUser.id).then(response => {\n      this.visits = response.data;\n    }, error => {\n      this.visits = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n  }\n};","map":{"version":3,"names":["UserService","VisitList","MyDialog","VisitForm","MyButton","visitService","components","name","data","visits","isVisitsLoading","dialogVisible","delVisit","methods","createVisit","visit","console","log","JSON","stringify","company_id","user_id","template_id","client_id","visit_date","description","then","response","error","message","toString","removeVisit","filter","v","id","showDialog","computed","currentUser","$store","state","auth","user","mounted","getVisitsByUser"],"sources":["C:\\Users\\Admin\\Desktop\\X\\SSTU\\6 семак\\БД\\DBCourseWork\\CW-front\\src\\components\\VisitsPage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Страница с визитами</h1>\r\n    <div class=\"visits__btns\">\r\n      <my-button @click=\"showDialog\"> Создать визит </my-button>\r\n    </div>\r\n    <my-dialog :show=\"dialogVisible\">\r\n      <visit-form @create=\"createVisit\" />\r\n    </my-dialog>\r\n    <div class=\"container\">\r\n      <visit-list :visits=\"visits\" @remove=\"removeVisit\" />\r\n      <!-- <div v-else>Идет загрузка...</div> -->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserService from \"../services/user.service\";\r\nimport VisitList from \"./VisitList.vue\";\r\nimport MyDialog from \"./UI/MyDialog.vue\";\r\nimport VisitForm from \"./VisitForm.vue\";\r\nimport MyButton from \"./UI/MyButton.vue\";\r\nimport visitService from \"@/services/visit.service\";\r\n\r\nexport default {\r\n  components: { VisitList, VisitForm, MyDialog, MyButton },\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: \"Visits\",\r\n  data() {\r\n    return {\r\n      visits: [],\r\n      isVisitsLoading: false,\r\n      dialogVisible: false,\r\n      delVisit: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    createVisit(visit) {\r\n      //this.visits.push(visit);\r\n      console.log(JSON.stringify(visit));\r\n      //JSON.stringify(visit)\r\n      visitService.createVisit({company_id: visit.company_id, user_id: visit.user_id, template_id: this.visit.template_id, client_id: this.visit.client_id, visit_date: this.visit.visit_date, description: this.visit.description}).then(\r\n        (response) => {\r\n          console.log(response);\r\n        },\r\n        (error) => {\r\n          this.visits =\r\n            (error.response &&\r\n              error.response.data &&\r\n              error.response.data.message) ||\r\n            error.message ||\r\n            error.toString();\r\n        }\r\n      );\r\n\r\n      this.dialogVisible = false;\r\n    },\r\n    removeVisit(visit) {\r\n      this.visits = this.visits.filter((v) => v.id !== visit.id);\r\n    },\r\n    showDialog() {\r\n      this.dialogVisible = true;\r\n    },\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  mounted() {\r\n    UserService.getVisitsByUser(this.currentUser.id).then(\r\n      (response) => {\r\n        this.visits = response.data;\r\n      },\r\n      (error) => {\r\n        this.visits =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.visits__btns {\r\n  margin: 15px 0;\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n</style>"],"mappings":"AAiBA,OAAOA,WAAU,MAAO,0BAA0B;AAClD,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,YAAW,MAAO,0BAA0B;AAEnD,eAAe;EACbC,UAAU,EAAE;IAAEL,SAAS;IAAEE,SAAS;IAAED,QAAQ;IAAEE;EAAS,CAAC;EACxD;EACAG,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,eAAe,EAAE,KAAK;MACtBC,aAAa,EAAE,KAAK;MACpBC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAACC,KAAK,EAAE;MACjB;MACAC,OAAO,CAACC,GAAG,CAACC,IAAI,CAACC,SAAS,CAACJ,KAAK,CAAC,CAAC;MAClC;MACAV,YAAY,CAACS,WAAW,CAAC;QAACM,UAAU,EAAEL,KAAK,CAACK,UAAU;QAAEC,OAAO,EAAEN,KAAK,CAACM,OAAO;QAAEC,WAAW,EAAE,IAAI,CAACP,KAAK,CAACO,WAAW;QAAEC,SAAS,EAAE,IAAI,CAACR,KAAK